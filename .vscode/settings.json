{
	"docker.commands.attach": "${containerCommand} exec -it ${containerId} fish",
	"terminal.integrated.rightClickBehavior": "nothing",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"todo-tree.highlights.customHighlight": {
		"LAY": {
			"background": "#30c3e7",
			"foreground": "white",
			"icon": "browser",
			"rulerColour": "blue",
			"iconColour": "blue",
			"gutterIcon": true
		},
		"INFO": {
			"background": "#bd41e2",
			"foreground": "white",
			// "icon": "browser",
			"rulerColour": "blue",
			"iconColour": "blue",
			"gutterIcon": true
		}
	},
	"todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO", "LAY", "INFO"],
	"terminal.integrated.windowsEnableConpty": true
}
